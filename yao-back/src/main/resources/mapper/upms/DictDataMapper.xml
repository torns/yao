<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.y3tu.yao.upms.mapper.DictDataMapper">

    <!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.y3tu.yao.upms.model.entity.DictData" id="dicdataMap">
        <result property="id" column="id"/>
        <result property="createBy" column="create_by"/>
        <result property="createTime" column="create_time"/>
        <result property="delFlag" column="del_flag"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateTime" column="update_time"/>
        <result property="description" column="description"/>
        <result property="dictId" column="dict_id"/>
        <result property="sort" column="sort"/>
        <result property="status" column="status"/>
        <result property="name" column="name"/>
        <result property="value" column="value"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id
        as
        id
        ,
        create_by
        as
        createBy,
        create_time
        as
        createTime,
        del_flag
        as
        delFlag,
        update_by
        as
        updateBy,
        update_time
        as
        updateTime,
        description
        as
        description,
        dict_id
        as
        dictId,
        sort
        as
        sort,
        status
        as
        status,
        name
        as
        name,
        value
    </sql>


    <select id="page" resultType="com.y3tu.yao.upms.model.entity.DictData">
        select
        <include refid="Base_Column_List"/>
        from t_dict_data
        where 1=1
        <if test="params.dictId!=null and params.dictId!=''">
            and dict_id = #{params.dictId}
        </if>
    </select>
</mapper>
